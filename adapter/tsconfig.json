{
    "compilerOptions": {
      "target": "es6",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "declaration": true,
      "outDir": "./lib",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": false
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }